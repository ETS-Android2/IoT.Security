<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".DetailFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <!--1이름 행-->
            <LinearLayout
                android:id="@+id/name_tab"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:text="Name" />
                    <TextView
                        android:id="@+id/name_content"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="put Something" />
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/checkbox1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp">
                </androidx.appcompat.widget.AppCompatCheckBox>
            </LinearLayout>
            <View
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#7E7E7E" />

            <!--2카테고리 행-->
            <LinearLayout
                android:id="@+id/category_tab"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/category"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:text="Category" />
                    <TextView
                        android:id="@+id/category_content"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="put Something" />
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/checkbox2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp">
                </androidx.appcompat.widget.AppCompatCheckBox>
            </LinearLayout>
            <View
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#7E7E7E" />

            <!--3장치 제공자 행-->
            <LinearLayout
                android:id="@+id/provider_tab"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/provider"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:text="Provider" />
                    <TextView
                        android:id="@+id/provider_content"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="put Something" />
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/checkbox3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp">
                </androidx.appcompat.widget.AppCompatCheckBox>
            </LinearLayout>
            <View
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#7E7E7E" />

            <!--4데이터 행-->
            <LinearLayout
                android:id="@+id/data_tab"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/data"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:text="Data" />
                    <TextView
                        android:id="@+id/data_content"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="put Something" />
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/checkbox4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp">
                </androidx.appcompat.widget.AppCompatCheckBox>
            </LinearLayout>
            <View
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#7E7E7E" />

            <!--5제품명-->
            <LinearLayout
                android:id="@+id/product_name_tap"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/product_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:text="Product Name" />
                    <TextView
                        android:id="@+id/product_name_content"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="put Something" />
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/checkbox5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp">
                </androidx.appcompat.widget.AppCompatCheckBox>
            </LinearLayout>
            <View
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#7E7E7E" />

            <!--6모델번호-->
            <LinearLayout
                android:id="@+id/model_id_tap"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/model_id"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:text="Model ID" />
                    <TextView
                        android:id="@+id/model_id_content"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="put Something" />
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/checkbox6
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp">
                </androidx.appcompat.widget.AppCompatCheckBox>
            </LinearLayout>
            <View
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#7E7E7E" />

            <!--7cycle-->
            <LinearLayout
                android:id="@+id/cycle_tap"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/cycle"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:text="Cycle" />
                    <TextView
                        android:id="@+id/cycle_content"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="put Something" />
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/checkbox7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp">
                </androidx.appcompat.widget.AppCompatCheckBox>
            </LinearLayout>
            <View
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#7E7E7E" />

            <!--8수집 주기-->
            <LinearLayout
                android:id="@+id/period_tap"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/period"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:text="Period" />
                    <TextView
                        android:id="@+id/period_content"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="put Something" />
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/checkbox8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp">
                </androidx.appcompat.widget.AppCompatCheckBox>
            </LinearLayout>
            <View
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#7E7E7E" />

            <!--9디스플레이-->
            <LinearLayout
                android:id="@+id/display_tap"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/display"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:text="Display" />
                    <TextView
                        android:id="@+id/display_content"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="put Something" />
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/checkbox9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp">
                </androidx.appcompat.widget.AppCompatCheckBox>
            </LinearLayout>
            <View
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#7E7E7E" />

            <!--10이동성-->
            <LinearLayout
                android:id="@+id/portable_tap"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/portable"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:text="Portable" />
                    <TextView
                        android:id="@+id/portable_content"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="put Something" />
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/checkbox10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp">
                </androidx.appcompat.widget.AppCompatCheckBox>
            </LinearLayout>
            <View
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#7E7E7E" />

            <!--11동의 필요 여부-->
            <LinearLayout
                android:id="@+id/agree_tap"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/agree"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:text="Agree Requirement" />
                    <TextView
                        android:id="@+id/agree_content"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="put Something" />
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/checkbox11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp">
                </androidx.appcompat.widget.AppCompatCheckBox>
            </LinearLayout>
            <View
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#7E7E7E" />

        <!--12디바이스 타입-->
        <LinearLayout
            android:id="@+id/device_type_tap"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="320dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/device_type"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:text="Device Type" />
                <TextView
                    android:id="@+id/device_type_content"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="put Something" />
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatCheckBox
                android:id="@+id/checkbox12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp">
            </androidx.appcompat.widget.AppCompatCheckBox>
        </LinearLayout>
        <View
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#7E7E7E" />


        <!--13수집 방법-->
        <LinearLayout
            android:id="@+id/always_tap"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="320dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/always"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:text="Collecting Method" />
                <TextView
                    android:id="@+id/always"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="put Something" />
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatCheckBox
                android:id="@+id/checkbox13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp">
            </androidx.appcompat.widget.AppCompatCheckBox>
        </LinearLayout>
        <View
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#7E7E7E" />

        <!--14서비스 형태-->
        <LinearLayout
            android:id="@+id/service_type_tap"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="320dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/service_type"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:text="Service Type" />
                <TextView
                    android:id="@+id/service_type_content"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="put Something" />
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatCheckBox
                android:id="@+id/checkbox14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp">
            </androidx.appcompat.widget.AppCompatCheckBox>
        </LinearLayout>
        <View
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#7E7E7E" />


        <!--15정보 형태-->
        <LinearLayout
            android:id="@+id/info_type_tap"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="320dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/info_type"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:text="Information Type" />
                <TextView
                    android:id="@+id/info_type_content"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="put Something" />
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatCheckBox
                android:id="@+id/checkbox15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp">
            </androidx.appcompat.widget.AppCompatCheckBox>
        </LinearLayout>
        <View
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#7E7E7E" />


        <!--16연결 방식-->
        <LinearLayout
            android:id="@+id/connection_tap"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="320dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/connection"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:text="Connecting Method" />
                <TextView
                    android:id="@+id/connection_method"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="put Something" />
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatCheckBox
                android:id="@+id/checkbox16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp">
            </androidx.appcompat.widget.AppCompatCheckBox>
        </LinearLayout>
        <View
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#7E7E7E" />

        <!--17제품 번호-->
        <LinearLayout
            android:id="@+id/pi_id_tap"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="320dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/pi_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:text="Pi ID" />
                <TextView
                    android:id="@+id/pi_id_content"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="put Something" />
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatCheckBox
                android:id="@+id/checkbox17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp">
            </androidx.appcompat.widget.AppCompatCheckBox>
        </LinearLayout>
        <View
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#7E7E7E" />
        </LinearLayout>
    </ScrollView>
</FrameLayout>